{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 500,
  "height": 400,
  "data": {"url": "https://vega.github.io/editor/data/unemployment-across-industries.json"},
  "layer": [
    {
      "mark": {
        "type": "area"
      },
      "encoding": {
        "x": {
          "timeUnit": "yearmonth",
          "field": "date",
          "type": "temporal",
          "axis": {"format": "%Y"}
        },
        "y": {
          "aggregate": "sum",
          "field": "count",
          "type": "quantitative",
          "axis": {"title": "count", "orient": "left"}
        },
        "color": {
          "type": "quantitative",
          "scale": {
            "range": ["#1f77b4"]
          }
        }
      }
    }
  ]
}